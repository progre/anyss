{
  // Remote controller on browser
  //
  // Example, when set 26977, try access http://localhost:26977
  "remoteControllerPort": 26977,

  // Voice chat key in game.
  // Some symbols can not be used.
  //
  // Example:
  //   Steam  --> ["Alt"]
  //   PUBG   --> ["T"]
  //   Deceit --> ["Alt", "V"]
  "playKeys": [],

  // Output target device name
  // Use [tray icon] -> [Export all devices to clipboard]
  //
  // Example:
  //   VB-Audio Virtual Cable   --> "CABLE Input (VB-Audio Virtual Cable)"
  //     https://www.vb-audio.com/Cable/index.htm
  //   Yamaha NETDUETTO Driver  --> "Line (Yamaha NETDUETTO Driver (WDM))"
  //     http://netduetto.net/
  "outputDevice": "",

  // Modifier key to select sounds
  // If empty then disable global shortcut keys
  //
  // Example:
  //   "Shift"
  //   "Control+Shift"
  "modifierKey": "",

  // Sounds and keys
  //
  // Example:
  // {
  //   "fileName": "sound1.wav",
  //   "combinationKey": "A",    // option
  //   "tags": [                 // option
  //     "deceit",
  //     "blood"
  //   ]
  // }
  "sounds": []
}